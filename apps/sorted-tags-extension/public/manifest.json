{
  "manifest_version": 3,
  "name": "Tabs Sorter",
  "version": "0.1.1",
  "description": "Sort and manage your browser tabs",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/web-browser.png",
      "48": "icons/web-browser.png",
      "128": "icons/web-browser.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
