{
    "manifest_version": 3,
    "name": "Tab Sorter",
    "version": "1.0",
    "description": "Sort and manage your browser tabs",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/tag.png",
        "48": "icons/tag.png",
        "128": "icons/tag.png"
      }
    }
  }
  